{
  "name": "@statsig/statsig-ai",
  "version": "0.0.1-beta.8",
  "main": "./dist/index.js",
  "files": [
    "dist/",
    "examples/",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./otel": { 
      "import": "./dist/otel/index.js",
      "require": "./dist/otel/index.js",
      "types": "./dist/otel/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      ".": ["./dist/index.d.ts"] ,
      "otel": ["./dist/otel/index.d.ts"]
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "prepare": "rm -rf dist/ && tsc",
    "test": "npm run prepare && jest --testPathIgnorePatterns='src/__tests__/ai-tests' --forceExit",
    "test:ai": "npm run prepare && jest --testPathPattern=src/__tests__/ai-tests/.*\\.test\\.ts --forceExit",
    "lint": "eslint '*/**/*.{ts,tsx}' --fix --max-warnings 0 --cache --cache-strategy content && git status",
    "build:examples": "rm -rf examples/dist && tsc -p tsconfig.examples.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "repository": "https://github.com/statsig-io/statsig-ai-node.git",
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.62.0",
    "@opentelemetry/context-async-hooks": "^2.2.0",
    "@opentelemetry/core": "^2.0.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.203.0",
    "@opentelemetry/resources": "^2.0.1",
    "@opentelemetry/sdk-metrics": "^2.2.0",
    "@opentelemetry/sdk-node": "^0.203.0",
    "@opentelemetry/sdk-trace-base": "^2.2.0",
    "@opentelemetry/sdk-trace-node": "^2.2.0",
    "@opentelemetry/semantic-conventions": "^1.36.0",
    "@statsig/statsig-node-core": "*",
    "node-fetch": "^3.3.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "compression": "^1.7.5",
    "eslint": "^9.19.0",
    "express": "^4.21.2",
    "jest": "^29.7.0",
    "openai": "^4.84.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">= 6.14.2 < 7 || >= 8.11.2 < 9 || >= 9.11.0 < 10 || >= 10.0.0"
  },
  "bugs": {
    "url": "https://github.com/statsig-io/statsig-ai-node/issues"
  },
  "homepage": "https://github.com/statsig-io/statsig-ai-node#readme"
}
